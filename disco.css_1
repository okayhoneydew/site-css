.Spinner {
    margin: 100px auto;
    width: 40px;
    height: 40px;
    position: relative
}

.Spinner.red {
    animation: Spinner_Pulse 1.8s ease-in-out infinite;
    animation-delay: .5s
}

.Spinner.red .Spinner_Cube1,.Spinner.red .Spinner_Cube2 {
    background-color: color(#eb3836 tint(40%))
}

.Spinner.paused .Spinner_Cube1,.Spinner.paused .Spinner_Cube2 {
    animation-play-state: paused
}

.Spinner_Caption {
    position: absolute;
    left: 0;
    top: 40px;
    width: 100%;
    text-align: center
}

.Spinner_Cube1,.Spinner_Cube2 {
    background-color: #84dbb4;
    width: 15px;
    height: 15px;
    position: absolute;
    top: 0;
    left: 0;
    animation: Spinner_Cubemove 1.8s ease-in-out infinite;
    transition: background-color .3s
}

.Spinner_Cube2 {
    animation-delay: -.9s
}

@keyframes Spinner_Cubemove {
    25% {
        transform: translateX(42px) rotate(-90deg) scale(.5)
    }

    50% {
        transform: translateX(42px) translateY(42px) rotate(-179deg)
    }

    50.1% {
        transform: translateX(42px) translateY(42px) rotate(-180deg)
    }

    75% {
        transform: translateX(0) translateY(42px) rotate(-270deg) scale(.5)
    }

    to {
        transform: rotate(-1turn)
    }
}

@keyframes Spinner_Pulse {
    0%,85% {
        opacity: 1
    }

    45% {
        opacity: 0
    }
}

