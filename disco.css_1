    border-radius: 50%;
    background-color: #f7f7f7;
    background-size: 50%;
    background-position: 50%;
    background-repeat: no-repeat
}

.DownloadModal_Link .Icon.download {
    background-image: url(data:image/svg+xml;base64,)
}

.DownloadModal_Link .Icon.error {
    background-image: url(data:image/svg+xml;base64,)
}

.DownloadModal_Link .Icon.loading {
    background-image: url(data:image/svg+xml;base64,);
    animation: rotate 2s ease 0s infinite normal
}

.DownloadModal_Text {
    color: #666;
    font-weight: 600;
    padding: 0 0 .1rem;
    -webkit-text-decoration: none;
    text-decoration: none
}

.DownloadModal_Text.is-link {
    cursor: pointer;
    color: #357cb4;
    border-bottom: .1rem solid
}

.DownloadModal_Text.is-error {
    color: #eb3836;
    border-bottom: .1rem solid
}

@keyframes rotate {
    0% {
        transform: rotate(0deg)
    }

    to {
        transform: rotate(1turn)
    }
}

.Sticky {
    position: -webkit-sticky;
    position: sticky;
    top: 0
}

.e2col-tr-play {
    opacity: 0
}

.e2col-tr-play.is-playing {
    opacity: 1
}

.e2col-tr-play .PlayerBar2-icon {
    transition: transform 90ms
}

.e2col-tr-play:hover .PlayerBar2-icon {
    transform: scale(1.23)
}

.e2col-tr-play:hover .is-document .PlayerBar2-icon {
    transform: scale(1.2)
}

.e2col-tr-play :active .PlayerBar2-icon {
    transform: scale(1)
}

.PlayerBar2 {
    color: #fff
}

.is-phone .PlayerBar2.is-small {
    height: 1.3rem
}

.PlayerBar2--disabled {
    cursor: default
}

.LoadingIndicator {
    display: inline-flex;
    overflow: hidden;
    align-content: center;
    justify-content: center;
    place-content: center;
    align-items: center;
    justify-items: center;
    place-items: center;
    vertical-align: top
}

.LoadingIndicator--small {
    --icon-size: 16px;
    min-width: 24px;
    min-height: 24px
}

.LoadingIndicator--medium {
    --icon-size: 24px;
    min-width: 32px;
    min-height: 32px
}

.LoadingIndicator--large {
    --icon-size: 30px;
    min-width: 40px;
    min-height: 40px
}

.LoadingIndicator-icon {
    animation: LoadingIndicator-animation .5s linear infinite;
    fill: #a5adba;
    fill: currentcolor;
    width: var(--icon-size);
    height: var(--icon-size)
}

@keyframes LoadingIndicator-animation {
    to {
        transform: rotate(1turn)
    }
}

.CompactAccordion__TitleContainer {
    position: relative;
    font-weight: 400;
    font-size: 11px;
    line-height: 28px;
    color: #6b778c;
    cursor: pointer;
    display: flex;
    align-content: space-between;
    justify-content: space-between;
    place-content: space-between;
    align-items: center;
    justify-items: center;
    place-items: center;
    margin: 0;
    padding: 0 12px;
    height: 28px
}

.CompactAccordion__TitleContainer:before {
    content: "";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    top: -1px;
    border-top: 1px solid #f1f4f5;
    border-bottom: 1px solid #f1f4f5
}

.CompactAccordion__Title {
    order: 1;
    flex-grow: 1;
    display: flex;
    align-content: flex-start;
    justify-content: flex-start;
    place-content: flex-start;
    align-items: center;
    justify-items: center;
    place-items: center
}

.HighlightedDropTarget .CompactAccordion__TitleContainer {
    background: #eaf8f2
}

.CompactAccordion__Arrow {
    will-change: transform;
    transition-duration: .1s;
    transition-property: transform;
    transition-timing-function: ease-out;
    order: 3
}

.CompactAccordion__Arrow--Left {
    order: 0;
    margin-right: 4px
}

.CompactAccordion__Arrow--Open {
    transform: rotate(90deg)
}

.CompactAccordion__LoadingIcon {
    margin-left: 4px
}

.CompactAccordion__Button {
    order: 2;
    margin-right: 34px;
    position: relative
}

.CompactAccordion__TitleContainer--Gray {
    color: hsla(0,0%,100%,.6)
}

.CompactAccordion__TitleContainer--Gray:before {
    border-top-color: hsla(0,0%,100%,.25);
    border-bottom-color: hsla(0,0%,100%,.25)
}

.CompactAccordion__TitleContainer--Dark {
    color: rgba(0,0,0,.6)
}

.CompactAccordion__TitleContainer--Dark:before {
    border-top-color: rgba(0,0,0,.25);
    border-bottom-color: rgba(0,0,0,.25)
}

.CompactAccordion__TitleContainer--HideBorderTop:after {
    border-top: 0
}

.CompactAccordion__TitleContainer--NodeLeft {
    padding-left: 0
}

.CompactAccordion__NodeLeft {
    height: 100%;
    order: -1;
    margin-right: 12px
}

.selectable-track-formats {
