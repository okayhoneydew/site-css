.switch {
    --background-checked: #32b57c;
    --background-default: #6b778c;
    display: inline-flex;
    align-items: center;
    cursor: pointer;
    min-height: 32px
}

.switch--disabled {
    --background-checked: #f4f4f4;
    --background-default: #f4f4f4
}

.switch-mask {
    border-radius: 9px;
    color: #fff;
    overflow: hidden;
    padding: 2px;
    position: relative;
    will-change: transform;
    width: 32px;
    height: 18px
}

.switch-mask:before {
    content: "";
    background: var(--background-default);
    will-change: transform;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%
}

.switch-input {
    opacity: 0;
    pointer-events: none;
    position: absolute
}

.switch-background {
    background: var(--background-checked);
    opacity: 0;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    transition-property: opacity;
    transition-duration: .1s;
    transition-timing-function: ease-in-out;
    will-change: opacity
}

.switch-input:checked+.switch-background {
    opacity: 1
}

.switch-toggle {
    background: #fff;
    border-radius: 50%;
    width: 50%;
    height: 100%;
    transition-property: transform;
    transition-duration: .1s;
    transition-timing-function: ease-in-out;
    will-change: transform
}

.switch-input:checked~.switch-toggle {
    transform: translateX(100%)
}

.switch-icon {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
    position: absolute;
    top: 0;
    transition-property: opacity;
    transition-delay: .1s;
    transition-duration: .3s;
    transition-timing-function: ease-in-out;
    will-change: opacity
}

.switch-icon--checked {
    opacity: 0;
    left: 2px;
    right: 50%
}

.switch-input:checked~.switch-icon--checked {
    opacity: 1
}

.switch-icon--unchecked {
    left: 50%;
    right: 2px
}

.switch-input:checked~.switch-icon--unchecked {
    opacity: 0
}

.switch-toggle--label {
    font-size: 14px;
    line-height: 20px;
    display: flex;
    align-items: end
}

.switch-toggle--container,.switch-toggle-container--disabled {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between
}

.switch-toggle-container--disabled {
    cursor: not-allowed
}

.switch-toggle-container--disabled .switch-toggle--label {
    opacity: .5
}

.switch-tool-tip--icon {
    display: inline-block;
    margin: 0 0 0 4px;
    transform: translateY(-4px)
}

